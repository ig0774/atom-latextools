{
  "name": "latextools",
  "main": "./lib/latextools",
  "version": "0.8.1",
  "description": "LaTeXTools for Atom: comprehensive support for LaTeX documents",
  "keywords": [
    "tex",
    "latex",
    "latexmk",
    "texlive",
    "latextools"
  ],
  "repository": "https://github.com/msiniscalchi/atom-latextools",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "cson-parser": "^1.3.3",
    "fuzzaldrin": "^2.1.0",
	  "esrever": "^0.2.0",
    "js-yaml": "^3.6.1",
    "key-path-helpers": "^0.4.0",
    "node-json-minify": "^1.0.0",
    "string-format": "^0.5.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    }
  },
  "consumedServices": {
    "snippets": {
      "versions": {
        ">= 0.1.0": "consumeSnippets"
      }
    }
  },
  "activationHooks": ["language-latex:grammar-used"]
}
